{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "verbatimModuleSyntax": true
  },
  "include": [
    "vite.config.ts",
    "iconSymbolsPlugin.ts"
  ]
}
